{
	"name": "shortn",
	"version": "0.1.0",
	"private": true,
	"description": "A non-innovative URL shortener",
	"author": "ismailgunayy",
	"license": "MIT",
	"repository": "https://github.com/ismailgunayy/shortn",
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"start": "turbo run start",
		"check-types": "turbo run check-types",
		"clean": "rimraf .turbo out; turbo clean",
		"format": "prettier . --write",
		"lint": "turbo run lint"
	},
	"devDependencies": {
		"prettier": "^3.6.2",
		"rimraf": "^6.0.1",
		"turbo": "^2.5.8"
	},
	"engines": {
		"node": "24.10.0"
	},
	"packageManager": "yarn@1.22.22",
	"workspaces": [
		"apps/*",
		"packages/*"
	]
