{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"globalEnv": ["DATABASE_URL"],
	"tasks": {
		"build": {
			"dependsOn": ["clean", "^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**", ".svelte-kit/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"start": {
			"cache": false,
			"persistent": true
		},
		"clean": {
			"cache": false
		},
		"lint": {
			"cache": false
		},
		"check-types": {
			"cache": false
		}
	}
}
